# vim: et ft=scala ts=4 sw=4

def mod-service-init = {
    !rm -rfv $DATABASE;
    sql r#"
        create table service (
            name text
        );
    "#;
    0
}

def service = {
    src script = fmt r#"
        insert into service (name) values ("%s");
    "# $args;
    !sqlite3 <$script $DATABASE
}

0
